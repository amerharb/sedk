@startmindmap

<style>
mindmapDiagram {
  .primitive {
    BackgroundColor lightgreen
  }
  .type {
    BackgroundColor #lightblue
  }
  .class {
    BackgroundColor #yellow
  }
  .null {
    BackgroundColor #green
  }
  .enum {
    BackgroundColor #violet
  }
  .enumItem {
    BackgroundColor #pink
  }
}
