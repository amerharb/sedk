sql.select().from(table1)
SELECT FROM "table1";

sql.select(col1).from(table1)
SELECT "col1" FROM "table1";

sql.selectAsteriskFrom(table1).where(col1.eq$('x'))
SELECT * FROM "table1" WHERE "col1" = $1;
[ "x" ]

sql.select(table1.ASTERISK).from(table1)
SELECT "table1".* FROM "table1";

sql.select(table1.ASTERISK, col1).from(table1)
SELECT "table1".*, "col1" FROM "table1";

sql.insertInto(table1, col1, col2).values$("x", "y").returning(table1.ASTERISK)
INSERT INTO "table1"("col1", "col2") VALUES($1, $2) RETURNING "table1".*;
[ "x", "y" ]

sql.update(table1).set(col1.eq$("x")).where(col2.eq$("y"))
UPDATE "table1" SET "col1" = $1 WHERE "col2" = $2;
[ "x", "y" ]
