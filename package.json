{
  "name": "sedk-postgres",
  "version": "0.15.0-next.3",
  "description": "Simple SQL builder and validator",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*.d.ts",
    "dist/src/**/*.js",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amerharb/sedk-postgres.git"
  },
  "homepage": "https://github.com/amerharb/sedk-postgres#readme",
  "url": "https://github.com/amerharb/sedk-postgres/issues",
  "email": "sedk@amerharb.com",
  "scripts": {
    "start": "node dist/src/index.js",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "build&start": "npm run build && npm run start",
    "test": "jest",
    "build&test": "npm run build && npm run test",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "keywords": [
    "SQL",
    "SQL Builder",
    "Postgresql",
    "Postgres",
    "sedk"
  ],
  "author": "Amer Harb",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.27.0",
    "jest": "^29.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "exclude": [
      "@types/jest",
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "jest",
      "ts-jest",
      "ts-node",
      "typescript"
    ]
  }
}
